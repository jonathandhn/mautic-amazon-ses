    "require": {
        "php": ">=8.1.0",
        "mautic/core-lib": "^5.1 || ^6.0"     
        "symfony/amazon-mailer": "^6.4",
        "teknasyon-teknoloji/aws-ses-notification": "^2.0"
    },
    "license": "GPL-3.0-or-later",
    "keywords": [
        "mautic",
        "plugin",
        "integration",
        "amazon",
        "ses"
    ],
    "extra": {
        "install-directory-name": "AmazonSESBundle"
    },
    "authors": [
        {
            "name": "Pablo Veintimilla",
            "email": "pabloveintimilla@gmail.com"
        },
        {
    "name": "Jonathan Dahan",
